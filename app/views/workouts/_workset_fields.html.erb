<div class='nested-fields'>
<%= f.input :movement_id, collection: Movement.order(:name), input_html: {class: "movement form-control" }, label: false %>
<%= f.input :exercise_id, collection: Movement.order(:name), as: :grouped_select, group_method: :exercises, input_html: {class: "exercise form-control"}, label: false %>
<%= f.input :weight, placeholder: 'Weight', input_html: {class: "form-control-sm"}, label: false %>
<%= f.input :repetitions, placeholder: 'Reps', input_html: {class: "form-control-sm"}, label: false %>
<%= f.input :rpe, placeholder: 'RPE', input_html: {class: "form-control-sm"}, label: false %>
<%= link_to_remove_association "remove set", f %>

</div>
